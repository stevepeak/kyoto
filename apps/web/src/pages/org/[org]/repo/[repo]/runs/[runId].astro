---
import Layout from '@/layouts/layout.astro'
import { RunApp } from '@/components/apps/run-app'

export const prerender = false

const { org, repo, runId } = Astro.params
---

<Layout title={`${org}/${repo} - Run ${runId}`}>
  <RunApp orgSlug={org!} repoName={repo!} runId={runId!} client:load />
</Layout>

