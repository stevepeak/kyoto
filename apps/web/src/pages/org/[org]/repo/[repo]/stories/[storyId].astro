---
import Layout from '@/layouts/layout.astro'
import { StoryApp } from '@/components/apps/story-app'

export const prerender = false

const { org, repo, storyId } = Astro.params
---

<Layout title={`${org}/${repo}#${storyId}`}>
  <StoryApp orgName={org!} repoName={repo!} storyId={storyId!} client:load />
</Layout>
