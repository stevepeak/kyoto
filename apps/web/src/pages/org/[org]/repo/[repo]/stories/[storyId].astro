---
import Layout from '@/layouts/layout.astro'
import { StoryPage } from '@/components/apps/story-page'

export const prerender = false

const { org, repo, storyId } = Astro.params
---

<Layout title={`${org}/${repo}#${storyId}`}>
  <StoryPage orgName={org!} repoName={repo!} storyId={storyId!} client:load />
</Layout>
