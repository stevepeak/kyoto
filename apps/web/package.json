{
  "name": "@app/web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Web application for the monorepo scaffold",
  "scripts": {
    "dev": "astro dev --port 3001",
    "build": "astro build",
    "preview": "touch .env && set -a && source .env && set +a && astro preview --port 3001",
    "astro": "astro",
    "postinstall": "astro sync",
    "clean": "rm -rf .astro",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@app/api": "workspace:^",
    "@app/db": "workspace:^",
    "@app/schemas": "workspace:^",
    "@app/utils": "workspace:^",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/astro": "^10.23.0",
    "@tanstack/react-query": "^5.90.5",
    "@tiptap/extension-link": "^3.10.7",
    "@tiptap/extension-placeholder": "^3.10.7",
    "@tiptap/markdown": "^3.10.7",
    "@tiptap/react": "^3.10.7",
    "@tiptap/starter-kit": "^3.10.7",
    "@trigger.dev/react-hooks": "^4.1.0",
    "@trigger.dev/sdk": "^4.0.6",
    "@trpc/client": "11.6.0",
    "@trpc/tanstack-react-query": "11.6.0",
    "@uiw/react-codemirror": "^4.25.3",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "kysely": "^0.28.8",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@astrojs/node": "^9.5.0",
    "@astrojs/react": "^4.4.0",
    "@astrojs/vercel": "^9.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.16",
    "@trpc/server": "11.6.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/unist": "^3.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "astro": "^5.15.1",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.16",
    "terser": "^5.44.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
