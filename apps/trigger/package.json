{
  "name": "@app/trigger",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Trigger.dev workflows for the application",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "npx trigger.dev@latest dev",
    "deploy": "npx trigger.dev@latest deploy",
    "typecheck": "tsc -b",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@app/agents": "workspace:*",
    "@app/config": "workspace:*",
    "@app/db": "workspace:*",
    "@daytonaio/sdk": "^0.113.1",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@sentry/node": "^10.23.0",
    "@sentry/opentelemetry": "^10.25.0",
    "@trigger.dev/sdk": "4.0.7",
    "p-map": "^7.0.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@sentry/esbuild-plugin": "^4.6.0",
    "@trigger.dev/build": "4.0.7",
    "@types/node": "^22.18.10",
    "typescript": "^5.9.3"
  }
}
