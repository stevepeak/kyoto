{
  "name": "@app/trigger",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Trigger.dev workflows for the application",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "dev": "npx trigger.dev@latest dev",
    "deploy": "npx trigger.dev@latest deploy",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@app/agents": "workspace:*",
    "@app/browserbase": "workspace:*",
    "@app/cache": "workspace:*",
    "@app/config": "workspace:*",
    "@app/daytona": "workspace:*",
    "@app/db": "workspace:*",
    "@app/posthog": "workspace:^",
    "@app/schemas": "workspace:*",
    "@browserbasehq/stagehand": "catalog:",
    "@daytonaio/sdk": "catalog:",
    "@octokit/auth-app": "catalog:",
    "@octokit/rest": "catalog:",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/sdk-node": "catalog:",
    "@sentry/node": "catalog:",
    "@sentry/opentelemetry": "catalog:",
    "@trigger.dev/sdk": "catalog:",
    "ai": "catalog:",
    "diff": "catalog:",
    "import-in-the-middle": "catalog:",
    "p-map": "catalog:",
    "ts-dedent": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@sentry/esbuild-plugin": "^4.6.0",
    "@trigger.dev/build": "catalog:",
    "@types/node": "^22.18.10",
    "typescript": "^5.9.3"
  }
}
