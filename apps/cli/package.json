{
  "name": "@app/cli",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Kyoto",
  "bin": {
    "kyoto": "./bin/run.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@ai-sdk/openai": "catalog:",
    "@app/agents": "workspace:^",
    "@app/schemas": "workspace:^",
    "@app/shell": "workspace:^",
    "@oclif/core": "catalog:",
    "@openrouter/ai-sdk-provider": "catalog:",
    "ai": "catalog:",
    "chalk": "catalog:",
    "execa": "catalog:",
    "inquirer": "^13.0.2",
    "object-treeify": "catalog:",
    "ora": "catalog:",
    "terminal-link": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "typescript": "^5.9.3",
    "tsconfig": "workspace:*"
  },
  "oclif": {
    "bin": "kyoto",
    "dirname": "kyoto",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  }
}
