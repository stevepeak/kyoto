    "commander": "^14.0.2",
    "execa": "catalog:",
    "ink": "^6.5.1",
    "ink-link": "^5.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.2.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@kyoto/eslint-config": "workspace:*",
    "@types/node": "catalog:",
