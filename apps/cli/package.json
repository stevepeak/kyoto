{
  "name": "@app/cli",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Kyoto",
  "bin": {
    "kyoto": "./bin/run.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && prettier --ignore-path='../../.prettierignore' --check .",
    "fix": "bunx eslint --fix --quiet . && prettier --ignore-path='../../.prettierignore' --log-level=silent --write ."
  },
  "dependencies": {
    "@ai-sdk/openai": "catalog:",
    "@app/agents": "workspace:^",
    "@app/schemas": "workspace:^",
    "@app/shell": "workspace:^",
    "@app/vectra": "workspace:^",
    "@oclif/core": "catalog:",
    "@openrouter/ai-sdk-provider": "catalog:",
    "@sentry/bun": "catalog:",
    "ai": "catalog:",
    "chalk": "catalog:",
    "execa": "catalog:",
    "inquirer": "^13.0.2",
    "object-treeify": "catalog:",
    "ora": "catalog:",
    "terminal-link": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@kyoto/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "openai": "^6.10.0",
    "tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "oclif": {
    "bin": "kyoto",
    "dirname": "kyoto",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  }
}
