{
  "name": "@usekyoto/cli",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "description": "Kyoto",
  "bin": {
    "kyoto": "./bin/kyoto.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/kyoto.js",
    "dist/**",
    "README.md"
  ],
  "engines": {
    "bun": ">=1"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "generate:version": "bun run scripts/generate-version.ts",
    "prebuild": "bun run scripts/generate-version.ts",
    "build": "rm -rf dist && tsc --noEmit false --outDir dist --rootDir src",
    "pretypecheck": "bun run scripts/generate-version.ts",
    "typecheck": "tsc --noEmit",
    "prepack": "bun run build",
    "pack:dry": "npm pack --dry-run",
    "publish:npm": "npm publish --access public",
    "lint": "eslint . && prettier --ignore-path='../../.prettierignore' --check .",
    "fix": "eslint --fix --quiet . && prettier --ignore-path='../../.prettierignore' --log-level=silent --write ."
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@app/agents": "workspace:^",
    "@app/shell": "workspace:^",
    "@app/types": "workspace:^",
    "@app/utils": "workspace:^",
    "@app/vectra": "workspace:^",
    "@openrouter/ai-sdk-provider": "catalog:",
    "ai": "catalog:",
    "commander": "^14.0.2",
    "execa": "catalog:",
    "ink": "^6.5.1",
    "ink-link": "^5.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "openai": "catalog:",
    "posthog-node": "catalog:",
    "react": "^19.2.1",
    "ts-dedent": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@kyoto/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "^19.2.2",
    "tsconfig": "workspace:*",
    "typescript": "catalog:"
  }
}
