{
  "name": "@app/auth",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "OAuth proxy service for authentication",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "vercel-build": "echo 'No build needed for Vercel serverless'",
    "typecheck": "tsc -b",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@app/db": "workspace:*",
    "better-auth": "^1.3.34",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
