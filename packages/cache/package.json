{
  "name": "@app/cache",
  "type": "module",
  "version": "0.0.1",
  "description": "Cache service for story evaluation evidence",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && prettier --ignore-path='../../.prettierignore' --check .",
    "fix": "bunx eslint --fix --quiet . && prettier --ignore-path='../../.prettierignore' --log-level=silent --write ."
  },
  "dependencies": {
    "@app/db": "workspace:*",
    "@app/schemas": "workspace:*",
    "@daytonaio/sdk": "catalog:",
    "kysely": "catalog:"
  },
  "devDependencies": {
    "@kyoto/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "tsconfig": "workspace:*",
    "typescript": "catalog:"
  }
}
