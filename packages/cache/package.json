{
  "name": "@app/cache",
  "type": "module",
  "version": "0.0.1",
  "description": "Cache service for story evaluation evidence",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "typecheck": "tsc -b",
    "lint": "eslint . --max-warnings=0 --cache --concurrency=2",
    "lint:fix": "eslint . --fix --cache --concurrency=2",
    "lint:ci": "eslint . --max-warnings=0 --cache --concurrency=1 --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@app/db": "workspace:*",
    "@app/agents": "workspace:*",
    "@app/schemas": "workspace:*",
    "@daytonaio/sdk": "^0.113.1",
    "drizzle-orm": "^0.36.4"
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "typescript": "^5.9.3"
  }
}
